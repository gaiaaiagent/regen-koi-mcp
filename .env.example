# Regen KOI MCP Configuration
# Copy this file to .env and update with your settings

# KOI Server Endpoint (default: Regen's hosted KOI infrastructure)
KOI_API_ENDPOINT=https://koi.regen.network/api

# Optional: API Key for authenticated access (provides higher rate limits)
# Request an API key at: https://regen.network/developers
KOI_API_KEY=

# Optional: Override default endpoints for self-hosted deployments
# KOI_SEARCH_ENDPOINT=http://localhost:8301/api/koi/query
# KOI_STATS_ENDPOINT=http://localhost:8301/api/koi/stats

# Apache Jena Configuration for Graph Queries
# SPARQL endpoint for knowledge graph queries
JENA_ENDPOINT=http://localhost:3030/koi/sparql

# OpenAI Configuration (for natural language to SPARQL conversion)
# Leave empty to use fallback keyword search
OPENAI_API_KEY=
GPT_MODEL=gpt-4o-mini

# MCP Server Configuration
MCP_SERVER_NAME=regen-koi
MCP_SERVER_VERSION=1.0.0

# Graph Database Configuration (Apache AGE)
# PostgreSQL database with Apache AGE extension for graph queries
GRAPH_DB_HOST=localhost
GRAPH_DB_PORT=5432
GRAPH_DB_NAME=eliza
GRAPH_DB_USER=postgres
GRAPH_DB_PASSWORD=postgres
GRAPH_NAME=regen_graph

# KOI Database Configuration (pgvector)
# PostgreSQL database with pgvector extension for semantic search
KOI_DB_HOST=localhost
KOI_DB_PORT=5432
KOI_DB_NAME=koi
KOI_DB_USER=postgres
KOI_DB_PASSWORD=postgres

# Query Router Configuration
ENTITY_SIMILARITY_THRESHOLD=0.15

# Unified Search Configuration
EMBEDDING_DIM=1536
RRF_K=60